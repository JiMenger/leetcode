class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        int i=0;
        int j=0;
        vector<int>return_nums;
        for(i=0;i<nums.size();i++){
            for(j=i+1;j<nums.size();j++){
                if (nums[i]+nums[j]== target){
                    return_nums.push_back(i);
                    return_nums.push_back(j);  
                    break;
                }
            }
            
        }
        return return_nums ;
    }
};
